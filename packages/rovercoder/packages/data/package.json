{
  "name": "@rovercoder/data",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@portfoliosharp/shared": "*",
    "date-fns": "^4.1.0"
  },
  "scripts": {
    "install-all": "npm install",
    "build": "(tsc || true) && (npm run assets:build-data || true) && (npm run assets:copy || true)",
    "dev": "(npm run dev:watch-assets) & (npm run dev:watch-build-assets-data)",
    "dev:watch-build-assets-data": "(npm run build) & (npx tsc-watch --noClear --onSuccess \"npm run assets:build-data\")",
    "dev:watch-assets": "npm run assets:watch-and-copy",
    "assets:build-data": "node ./dist/runners/site.data.builder.js || true",
    "assets:watch-and-copy": "npx chokidar './assets/**/*' -c \"npm run assets:copy\" --initial",
    "assets:copy": "rm -rf ../site/public/assets && cp -r ./assets ../site/public/"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "chokidar-cli": "^3.0.0",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3"
  }
}
