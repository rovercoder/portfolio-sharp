{
  "name": "@rovercoder/site",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install-all": "npm install",
    "dev": "(npm run build:scripts) && (npm run watch:scripts & astro dev --host)",
    "build": "(npm run build:scripts) && astro build",
    "build:scripts": "(tsc --project ./src/scripts) && (tsc --project ./src/templates/scripts)",
    "watch:scripts": "(tsc --watch --project ./src/scripts) & (tsc --watch --project ./src/templates/scripts)",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@portfoliosharp/shared": "*",
    "astro": "^5.16.4"
  },
  "devDependencies": {
    "sass": "^1.94.2",
    "uglify-js": "^3.19.3"
  }
}
